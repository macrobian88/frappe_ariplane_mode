 "client_script": "frappe.web_form.on(\"before_load\", () => {\n  const params = new URLSearchParams(window.location.search);\n  const flight = params.get(\"flight\");\n\n  if (flight) {\n    frappe.web_form.set_value(\"flight\", flight);\n\n    frappe.call({\n      method: \"frappe.client.get_value\",\n      args: {\n        doctype: \"Airplane Flight\",\n        filters: { name: flight },\n        fieldname: [\n          \"source_airport\",\n          \"destination_airport\",\n          \"date_of_departure\",\n          \"time_of_departure\",\n          \"duration\",\n          \"status\",\n          \"airplane\"  // optional if needed later\n        ]\n      },\n      callback: function (r) {\n        if (r.message) {\n          frappe.web_form.set_value(\"source_airport\", r.message.source_airport);\n          frappe.web_form.set_value(\"destination_airport\", r.message.destination_airport);\n          frappe.web_form.set_value(\"departure_date\", r.message.date_of_departure);\n          frappe.web_form.set_value(\"departure_time\", r.message.time_of_departure);\n          frappe.web_form.set_value(\"duration_of_flight\", r.message.duration);\n        }\n      }\n    });\n\n    // Optional: Get flight price from a custom field or Pricing table\n    frappe.call({\n      method: \"frappe.client.get_value\",\n      args: {\n        doctype: \"Airplane Flight\",\n        filters: { name: flight },\n        fieldname: \"flight_price\"\n      },\n      callback: function (r) {\n        if (r.message) {\n          frappe.web_form.set_value(\"flight_price\", r.message.flight_price);\n        }\n      }\n    });\n  }\n});\n",
